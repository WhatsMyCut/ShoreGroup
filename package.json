    "bootstrap3-native": "^1.0.11",
    "connected-react-router": "^6.3.1",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "formik": "^1.5.1",
    "history": "^4.9.0",
    "json-to-url": "1.0.5",
    "nserializejson": "1.0.2",
    "nval": "1.1.5",
    "nval-tippy": "1.0.34",
    "office-ui-fabric-react": "^6.164.8",
    "react": "^16.8.5",
